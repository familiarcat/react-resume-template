{
  "name": "bradygeorgen",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=9.8.1"
  },
  "resolutions": {
    "execa": "8.0.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "fix-dependencies": "node scripts/fix-dependencies.js",
    "amplify-build": "npm run fix-dependencies && ./scripts/amplify-build.sh",
    "start": "next start",
    "lint": "next lint",
    "sandbox-delete": "unset AWS_ACCESS_KEY_ID && unset AWS_SECRET_ACCESS_KEY && unset AWS_SESSION_TOKEN && export AWS_PROFILE=AmplifyUser && npx ampx sandbox delete --profile AmplifyUser",
    "sandbox": "unset AWS_ACCESS_KEY_ID && unset AWS_SECRET_ACCESS_KEY && unset AWS_SESSION_TOKEN && export AWS_PROFILE=AmplifyUser && npx ampx sandbox --profile AmplifyUser",
    "sandbox-deploy": "node scripts/deploy-sandbox.js",
    "generate-env": "node scripts/generate-env.js",
    "local-dev": "npm run sandbox && npm run generate-env && npm run dev",
    "ci-deploy": "CI=true node scripts/deploy-pipeline.js"
  },
  "dependencies": {
    "@aws-sdk/types": "^3.775.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.2.0",
    "@reactflow/core": "^11.11.4",
    "aws-amplify": "^6.13.5",
    "aws-cdk-lib": "^2.188.0",
    "classnames": "^2.5.1",

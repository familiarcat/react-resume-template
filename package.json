{
  "name": "bradygeorgen",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "resolutions": {
    "execa": "8.0.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/build-error-handler.js",
    "start": "next start",
    "lint": "next lint",

    "fix-dependencies": "node scripts/fix-dependencies.js",
    "postinstall": "npm run fix-dependencies",

    "secure-deploy": "npm run build && node scripts/fix-amplify-files.js",
    "secure-deploy:dev": "npm run build && node scripts/fix-amplify-files.js && node scripts/amplify-secure-deploy.js development",
    "secure-deploy:prod": "npm run build && node scripts/fix-amplify-files.js && node scripts/amplify-secure-deploy.js production",

    "amplify:fix-files": "node scripts/fix-amplify-files.js",
    "amplify:gen2:fix-files": "node scripts/fix-amplify-files.js",
    "amplify:gen2:cicd": "npm run build && npm run amplify:fix-files && npm run seed-data:prod",
    "amplify:fix-parcel": "node scripts/fix-parcel-watcher.js",
    "amplify:check-build": "node scripts/build-error-handler.js",

    "seed-data": "node scripts/seed-data.js",
    "seed-data:dev": "node scripts/seed-data.js development",
    "seed-data:prod": "node scripts/seed-data.js production",

    "sync-data:dev-to-prod": "node scripts/seed-data.js sync development production",
    "sync-data:prod-to-dev": "node scripts/seed-data.js sync production development",

    "aws:fix": "node scripts/aws-credential-fix.js",
    "aws:check": "node scripts/check-aws.js",

    "sandbox": "npx ampx sandbox",
    "sandbox:once": "npx ampx sandbox --once",
    "local-dev": "npm run sandbox && npm run dev",

    "amplify:generate-outputs": "npx ampx generate outputs",
    "amplify:generate-graphql": "npx ampx generate graphql-client-code",
    "amplify:pipeline-deploy": "npx ampx pipeline-deploy",

    "ci-deploy": "CI=true npm run secure-deploy && npm run seed-data",
    "verify-deploy": "node scripts/verify-amplify-deployment.js",

    "test": "jest --testMatch='<rootDir>/tests/**/*.test.(ts|tsx|js|jsx)'",
    "test:watch": "jest --testMatch='<rootDir>/tests/**/*.test.(ts|tsx|js|jsx)' --watch",
    "test:ci": "jest --testMatch='<rootDir>/tests/**/*.test.(ts|tsx|js|jsx)' --ci --coverage",
    "test:amplify": "jest --testPathPattern=tests/amplify",
    "test:components": "jest --testPathPattern=tests/components",
    "test:lib": "jest --testPathPattern=tests/lib",
    "test:actions": "jest --testPathPattern=tests/actions"
  },
  "dependencies": {
    "@aws-amplify/backend-graphql": "0.3.2",
    "@aws-amplify/graphql-model-transformer": "3.2.2",
    "@aws-sdk/client-dynamodb": "3.788.0",
    "@aws-sdk/lib-dynamodb": "3.788.0",
    "@aws-sdk/types": "^3.775.0",
    "@emotion/is-prop-valid": "1.3.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.2.0",
    "@reactflow/core": "^11.11.4",
    "aws-amplify": "^6.13.5",
    "aws-cdk-lib": "^2.188.0",
    "aws-sdk": "2.1692.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "constructs": "^10.4.2",
    "framer-motion": "^12.6.3",
    "next": "^14.1.0",
    "react": "19.1.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "19.1.0",
    "react-lottie-player": "^2.1.0",
    "reactflow": "^11.11.4",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "1.3.0",
    "@aws-amplify/backend-cli": "1.3.0",
    "@aws-amplify/cli": "12.10.0",
    "@aws-sdk/client-iam": "3.787.0",
    "@aws-sdk/credential-provider-ini": "3.787.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/classnames": "^2.3.4",
    "@types/ini": "4.1.1",
    "@types/next": "^9.0.0",
    "@types/node": "22.13.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "ampx": "0.2.2",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.2.3",
    "ini": "5.0.0",
    "postcss": "^8.4.31",
    "postcss-nesting": "^12.0.1",
    "postcss-preset-env": "^9.3.0",
    "sass": "^1.69.0",
    "serve": "14.2.4",
    "sharp": "0.34.1",
    "source-map-loader": "5.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "npm@9.8.1"
}
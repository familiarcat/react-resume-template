    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/classnames": "^2.3.4",
    "@types/next": "^9.0.0",
    "@types/node": "22.13.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "ampx": "0.2.2",
    "autoprefixer": "^10.4.16",

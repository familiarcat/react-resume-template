    "sandbox-deploy": "node scripts/deploy-sandbox.js",
    "generate-env": "node scripts/generate-env.js",
    "local-dev": "npm run sandbox && npm run generate-env && npm run dev",
    "ci-deploy": "CI=true node scripts/deploy-pipeline.js"
  },

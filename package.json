    "lint": "next lint",
    "sandbox-delete": "unset AWS_ACCESS_KEY_ID && unset AWS_SECRET_ACCESS_KEY && unset AWS_SESSION_TOKEN && export AWS_PROFILE=AmplifyUser && npx ampx sandbox delete --profile AmplifyUser --debug",

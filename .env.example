# AWS Credentials
# Use either AWS_PROFILE or AWS_ACCESS_KEY_ID/AWS_SECRET_ACCESS_KEY, not both
AWS_PROFILE=AmplifyUser
# AWS_ACCESS_KEY_ID=your-access-key-id
# AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-2

# Amplify Configuration
AMPLIFY_APP_ID=d28u81cjrxr0oe
BRANCH_NAME=main

# Amplify Public Configuration
NEXT_PUBLIC_USER_POOL_ID=your-user-pool-id
NEXT_PUBLIC_USER_POOL_CLIENT_ID=your-user-pool-client-id
NEXT_PUBLIC_GRAPHQL_ENDPOINT=https://your-api-id.appsync-api.us-east-2.amazonaws.com/graphql
NEXT_PUBLIC_API_KEY=your-api-key
NEXT_PUBLIC_S3_BUCKET=your-s3-bucket
NEXT_PUBLIC_AWS_REGION=us-east-2

# DynamoDB Local (for development)
DYNAMODB_LOCAL_ENDPOINT=http://localhost:20002

# Node Environment
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# Deployment
DEPLOYMENT_URL=https://main.your-app-id.amplifyapp.com
